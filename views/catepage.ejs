<ul>
  <% for (k in categories){ %>
  <li>
  <a href="<%= baseurl %>post/<%= categories[k].id %>">
  <% for (pro in categories[k]) { %>
  <%= categories[k][pro]%> 
  <% } %>
  </a>
  </li>
